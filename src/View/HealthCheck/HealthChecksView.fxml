<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="701.0" prefWidth="726.0" xmlns="http://javafx.com/javafx/8.0.202"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.HealthCheck.HealthChecksViewController">
   <TableView fx:id="tblUserHealthCheck" focusTraversable="false" layoutX="17.0" layoutY="70.0"
              onMouseClicked="#tblUserHealthCheckOnMouseClicked" prefHeight="566.0" prefWidth="691.0">
      <columns>
         <TableColumn fx:id="colFever" editable="false" prefWidth="131.0" sortType="DESCENDING" text="Fever"/>
         <TableColumn fx:id="colRespiratoryDisorder" editable="false" prefWidth="140.0" text="Respiratory Disorder"/>
         <TableColumn fx:id="colSmellTasteDisorder" editable="false" prefWidth="181.0" text="Smell and Taste Disorder"/>
         <TableColumn fx:id="colDateResult" editable="false" prefWidth="173.0" resizable="false" sortType="DESCENDING"
                      text="Date Result"/>
      </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
   </TableView>
   <Button fx:id="btnToUsers" focusTraversable="false" layoutX="17.0" layoutY="20.0" mnemonicParsing="false"
           onAction="#btnToUsersOnAction" prefHeight="30.0" prefWidth="149.0" text="&lt;- All Users"/>
   <Button fx:id="btnAddUserHealthCheck" focusTraversable="false" layoutX="113.0" layoutY="648.0"
           mnemonicParsing="false" onAction="#btnAddUserHealthCheckOnAction" prefHeight="39.0" prefWidth="149.0"
           text="Add Health Check"/>
   <Button fx:id="btnEditUserHealthCheck" focusTraversable="false" layoutX="289.0" layoutY="648.0"
           mnemonicParsing="false" onAction="#btnEditUserHealthCheckOnAction" prefHeight="39.0" prefWidth="149.0"
           text="Edit Health Check"/>
   <Button fx:id="btnDeleteUserHealthCheck" focusTraversable="false" layoutX="463.0" layoutY="648.0"
           mnemonicParsing="false" onAction="#btnDeleteUserHealthCheckOnAction" prefHeight="39.0" prefWidth="149.0"
           text="Delete Health Check"/>
</AnchorPane>
