<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="683.0" prefWidth="779.0" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Location.LocationContactsViewController">
   <children>
      <TableView fx:id="tblLocationContacts" focusTraversable="false" layoutX="14.0" layoutY="58.0" onMouseClicked="#tblContactsOnMouseClicked" prefHeight="548.0" prefWidth="752.0">
         <columns>
            <TableColumn fx:id="colUser1" prefWidth="300.0" resizable="false" text="User 1">
               <columns>
                  <TableColumn fx:id="colUser1UserName" prefWidth="92.0" sortType="DESCENDING" text="Username" />
                  <TableColumn fx:id="colUser1FirstName" editable="false" prefWidth="102.0" resizable="false" sortType="DESCENDING" text="First Name" />
                  <TableColumn fx:id="colUser1LastName" editable="false" prefWidth="101.0" resizable="false" sortType="DESCENDING" text="Last Name" />
               </columns>
            </TableColumn>
            <TableColumn fx:id="colUser2" editable="false" prefWidth="257.0" resizable="false" text="User 2">
               <columns>
                  <TableColumn fx:id="colUser2UserName" editable="false" prefWidth="75.0" text="Username" />
                  <TableColumn fx:id="colUser2FirstName" editable="false" prefWidth="75.0" text="First Name" />
                  <TableColumn fx:id="colUser2LastName" editable="false" prefWidth="75.0" text="Last Name" />
               </columns>
            </TableColumn>
            <TableColumn fx:id="colDateReceived" editable="false" prefWidth="126.0" resizable="false" text="Date" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnAddContact" focusTraversable="false" layoutX="136.0" layoutY="621.0" mnemonicParsing="false" onAction="#btnAddContactOnAction" prefHeight="39.0" prefWidth="149.0" text="Add Contact" />
      <Button fx:id="btnEditContact" focusTraversable="false" layoutX="316.0" layoutY="621.0" mnemonicParsing="false" onAction="#btnEditContactOnAction" prefHeight="39.0" prefWidth="149.0" text="Edit Contact" />
      <Button fx:id="btnDeleteContact" focusTraversable="false" layoutX="496.0" layoutY="621.0" mnemonicParsing="false" onAction="#btnDeleteContactOnAction" prefHeight="39.0" prefWidth="149.0" text="Delete Contact" textFill="#cd0000" />
      <Button fx:id="btnToLocations" focusTraversable="false" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnToLocationsOnAction" prefHeight="30.0" prefWidth="149.0" text="&lt;- All Locations" />
   </children>
</AnchorPane>
