<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="624.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Contact.ContactAddSecondUserViewController">
   <children>
      <TableView fx:id="tblUser" focusTraversable="false" layoutX="15.0" layoutY="37.0" onMouseClicked="#tblUserOnMouseClicked" prefHeight="533.0" prefWidth="570.0">
         <columns>
            <TableColumn fx:id="colUserName" prefWidth="92.0" sortType="DESCENDING" text="Username" />
            <TableColumn fx:id="colFirstName" editable="false" prefWidth="102.0" resizable="false" sortType="DESCENDING" text="First Name" />
            <TableColumn fx:id="colLastName" editable="false" prefWidth="101.0" resizable="false" sortType="DESCENDING" text="Last Name" />
            <TableColumn fx:id="colGender" editable="false" prefWidth="63.0" resizable="false" sortType="DESCENDING" text="Gender" />
            <TableColumn fx:id="colDateOfBirth" editable="false" minWidth="0.0" prefWidth="100.0" resizable="false" sortType="DESCENDING" text="Date of birth" />
            <TableColumn fx:id="colDateOfDeath" editable="false" prefWidth="111.0" resizable="false" sortType="DESCENDING" text="Date of death" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnNext" layoutX="429.0" layoutY="585.0" mnemonicParsing="false" onAction="#btnNextOnAction" prefHeight="25.0" prefWidth="70.0" text="Next" />
      <Button fx:id="btnCancel" layoutX="515.0" layoutY="585.0" mnemonicParsing="false" onAction="#btnCancelOnAction" prefHeight="25.0" prefWidth="70.0" text="Cancel" />
      <Label layoutX="15.0" layoutY="14.0" text="Pick a user from the table:" />
   </children>
</AnchorPane>
