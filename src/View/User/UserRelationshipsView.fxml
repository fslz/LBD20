<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="701.0" prefWidth="634.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.User.UserRelationshipsViewController">
   <children>
      <TableView fx:id="tblUserRelationships" focusTraversable="false" layoutX="19.0" layoutY="69.0" onMouseClicked="#tblUserContactsOnMouseClicked" prefHeight="566.0" prefWidth="596.0">
         <columns>
            <TableColumn fx:id="colUser" editable="false" prefWidth="317.0" sortType="DESCENDING" text="User">
               <columns>
                  <TableColumn fx:id="colUserName" editable="false" prefWidth="64.0" text="Username" />
                  <TableColumn fx:id="colFirstName" editable="false" prefWidth="105.0" resizable="false" sortType="DESCENDING" text="First Name" />
                  <TableColumn fx:id="colLastName" editable="false" prefWidth="113.0" resizable="false" sortType="DESCENDING" text="Last Name" />
               </columns>
            </TableColumn>
            <TableColumn fx:id="colRelationshipType" editable="false" prefWidth="238.0" resizable="false" sortType="DESCENDING" text="Relationship Type" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnToUsers" focusTraversable="false" layoutX="19.0" layoutY="24.0" mnemonicParsing="false" onAction="#btnToUsersOnAction" prefHeight="30.0" prefWidth="149.0" text="&lt;- All Users" />
      <Button fx:id="btnAddUserRelationship" focusTraversable="false" layoutX="68.0" layoutY="648.0" mnemonicParsing="false" onAction="#btnAddUserRelationshipOnAction" prefHeight="39.0" prefWidth="149.0" text="Add Relationship" />
      <Button fx:id="btnEditUserRelationship" focusTraversable="false" layoutX="244.0" layoutY="648.0" mnemonicParsing="false" onAction="#btnEditUserRelationshipOnAction" prefHeight="39.0" prefWidth="149.0" text="Edit Relationship" />
      <Button fx:id="btnDeleteUserRelationship" focusTraversable="false" layoutX="418.0" layoutY="648.0" mnemonicParsing="false" onAction="#btnDeleteUserRelationshipOnAction" prefHeight="39.0" prefWidth="149.0" text="Delete Relationship" />
   </children>
</AnchorPane>
